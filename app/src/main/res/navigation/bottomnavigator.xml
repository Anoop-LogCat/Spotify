<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottomnavigator"
    app:startDestination="@id/home2">

    <fragment
        android:id="@+id/home2"
        android:name="production.binarycoder.spotify.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home2_to_albumFragment2"
            app:destination="@id/albumFragment2" />
    </fragment>
    <fragment
        android:id="@+id/search"
        android:name="production.binarycoder.spotify.Search"
        android:label="fragment_search"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/library"
        android:name="production.binarycoder.spotify.Library"
        android:label="fragment_library"
        tools:layout="@layout/fragment_library" />
    <fragment
        android:id="@+id/premium"
        android:name="production.binarycoder.spotify.Premium"
        android:label="fragment_premium"
        tools:layout="@layout/fragment_premium" />
    <fragment
        android:id="@+id/albumFragment2"
        android:name="production.binarycoder.spotify.AlbumFragment"
        android:label="fragment_album"
        tools:layout="@layout/fragment_album" />
</navigation>